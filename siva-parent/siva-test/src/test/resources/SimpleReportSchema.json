{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "validationTime": {
      "type": "string"
    },
    "signaturesCount": {
      "type": "integer"
    },
    "documentName": {
      "type": "string"
    },
    "validSignaturesCount": {
      "type": "integer"
    },
    "signatures": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "signatureFormat": {
            "type": "string"
          },
          "signedBy": {
            "type": "string"
          },
          "claimedSigningTime": {
            "type": "string"
          },
          "signatureLevel": {
            "type": "string"
          },
          "warnings": {
            "type": "array",
            "items": {
              "properties": {
                "nameId": {
                  "type": "string"
                },
                "description": {
                  "type": "string"
                }
              }
            }
          },
          "signatureScopes": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "scope": {
                  "type": "string"
                },
                "name": {
                  "type": "string"
                },
                "content": {
                  "type": "string"
                }
              },
              "required": [
                "scope",
                "name",
                "content"
              ]
            }
          },
          "id": {
            "type": "string"
          },
          "indication": {
            "type": "string"
          },
          "errors": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "certificateId": {
                },
                "nameId": {
                },
                "content": {
                }
              }
            }
          },
          "info": {
            "type": "object",
            "properties": {
              "nameId": {
                "type": "string"
              },
              "bestSignatureTime": {
                "type": "string"
              }
            },
            "required": [
              "nameId",
              "bestSignatureTime"
            ]
          }
        },
        "required": [
          "signatureFormat",
          "signedBy",
          "claimedSigningTime",
          "signatureLevel",
          "warnings",
          "signatureScopes",
          "id",
          "indication",
          "errors",
          "info"
        ]
      }
    },
    "policy": {
      "type": "object",
      "properties": {
        "policyDescription": {
          "type": "string"
        },
        "policyUrl": {
          "type": "string"
        },
        "policyName": {
          "type": "string"
        }
      },
      "required": [
        "policyDescription",
        "policyUrl",
        "policyName"
      ]
    }
  },
  "required": [
    "validationTime",
    "signaturesCount",
    "documentName",
    "validSignaturesCount",
    "signatures",
    "policy"
  ]
}